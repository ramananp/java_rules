<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jee="http://www.springframework.org/schema/jee"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
       
       	<bean id="jsonProvider" scope="singleton"
			class="com.jpmorgan.pb.rest.jersey.JaxbJsonProvider" />
	
		
		<bean id="pingService" class="com.jpmchase.aata.services.core.AATAPingService"></bean>
		<bean id="creditGatewayService" class="com.jpmchase.aata.services.core.AATACreditGatewayService"></bean>	
		<bean id="commentService" class="com.jpmchase.aata.services.core.AATACommentsService">
		
        	<property name="commentsBO">
            	<ref bean="commentsBO"/>
        	</property>
        	<property name="attachmentBO">
            	<ref bean="attachmentBO"/>
        	</property>
        	<property name="contextLoader" ref="contextLoader" />
        	<property name="aataAppContextInfo" ref="aataAppContextInfo" />
		</bean>
		<bean id="aataworkflowservice" class="com.jpmchase.aata.services.core.AATAWorkflowService">
        	
        	<property name="workFlowBO">
            	<ref bean="workFlowBO"/>
        	</property>
		</bean>
		
		<bean id="aataStandardService" class="com.jpmchase.aata.services.core.AATAStandardService">
        	<property name="aataStandardBO" ref="aataStandardBO"/>
        	<property name="workFlowBO">
            	<ref bean="workFlowBO"/>
        	</property>
		</bean>
		<bean id="aataFreeDeliveryService" class="com.jpmchase.aata.services.core.AATAFreeDeliveryService">
        	<property name="aataFreeDeliveryBO" ref="aataFreeDeliveryBO"/>
        	<property name="workFlowBO">
            	<ref bean="workFlowBO"/>
        	</property>
		</bean>
		<bean id="aataFreeReceiveService" class="com.jpmchase.aata.services.core.AATAFreeReceiveService">
        	<property name="aataFreeReceiveBO" ref="aataFreeReceiveBO"/>
        	<property name="workFlowBO">
            	<ref bean="workFlowBO"/>
        	</property>
        	<property name="aataFreeDeliveryBO">
            	<ref bean="aataFreeDeliveryBO"/>
        	</property>
		</bean>
		<bean id="aataGiftService" class="com.jpmchase.aata.services.core.AATAGiftService">
        	<property name="aataGiftBO" ref="aataGiftBO"/>
        	<property name="aataFreeDeliveryBO">
            	<ref bean="aataFreeDeliveryBO"/>
        	</property>
        	<property name="workFlowBO">
            	<ref bean="workFlowBO"/>
        	</property>
        	<property name="contextLoader" ref="contextLoader" />
		</bean>
		<bean id="aataStatusService" class="com.jpmchase.aata.services.core.AATAStatusService">
        	<property name="aataStatusBO" ref="aataStatusBO"/>
		</bean>
		<bean id="aataSanctionService" class="com.jpmchase.aata.services.core.AATASanctionService">
        	<property name="aataSanctionBO" ref="aataSanctionBO"/>
		</bean>
</beans>       
